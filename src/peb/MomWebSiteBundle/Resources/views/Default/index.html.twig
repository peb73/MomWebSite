{% extends 'pebMomWebSiteBundle::layout.html.twig' %}

{% set routing = 'default' %}

{% block content %}

    <div class="splash">
        {% include "pebMomWebSiteBundle:Default:splash.html.twig" %}
    </div>

    <div class="content">        

        {% for actualite in actualites %}
            {% include "pebMomWebSiteBundle:Default:actualite.html.twig" %}
        {% else %}
            {% include "noactualite.html.twig" %}
        {% endfor %}
    </div>

{% endblock %}